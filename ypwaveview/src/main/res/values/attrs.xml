<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="shapeType" format="enum">
        <enum name="circle" value="1" />
        <enum name="square" value="2" />
        <enum name="heart" value="3" />
    </attr>
    <attr name="progress" format="integer" />
    <attr name="max" format="integer" />
    <attr name="borderWidthSize" format="dimension" />
    <attr name="frontColor" format="color" />
    <attr name="behideColor" format="color" />
    <attr name="borderColor" format="color" />
    <attr name="strong" format="integer" />
    <attr name="textColor" format="color" />
    <attr name="animatorEnable" format="boolean" />

    <declare-styleable name="YPWaveView">
        <attr name="progress" />
        <attr name="max" />
        <attr name="strong" />
        <attr name="frontColor" />
        <attr name="behideColor" />
        <attr name="borderColor" />
        <attr name="borderWidthSize" />
        <attr name="textColor" />
        <attr name="animatorEnable" />
        <attr name="shapeType" />
    </declare-styleable>
</resources>